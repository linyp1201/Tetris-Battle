    method int create_block(Map A, Block B){
        do B.setR(0);
        do B.setC(4);
        let r = B.getR();
        let c = B.getC();
        if((A.read(r, c) = 1) | (A.read(r, c+1) = 1) | (A.read(r, c-1) = 1) | (A.read(r+1, c) = 1)){
            return 0;
        }
        do A.set(r,c);
        do A.set(r,c+1);
        do A.set(r,c-1);
        do A.set(r+1, c);
        let type = 0;
        return 1;
    }

    // Put into TBlock.jack

    method boolean T_rotate(Map M, Block B)
    {	
      
      var int r; 
      var int c; 
      let r = B.getR();
      let c = B.getC();

      if ((r< 19) & (c> 0) & (c< 9))
      {
        if (( M.read(r ,c)= 1 ) & ( M.read(r ,c-1)= 1 ) & ( M.read(r, c+1)= 1 ) & ( M.read(r+1 ,c)= 1 ))
          {let type= 0;}
      }
          
      if ((r> 0) & (r< 19) & (c> 0))
      {
        if (( M.read(r, c)= 1 ) & ( M.read(r-1 ,c)= 1 ) & ( M.read(r ,c-1)= 1 ) & ( M.read(r+1 ,c)= 1 ))
          {let type= 1;}
      }			
          
      if ((r> 0) & (c> 0) & (c< 9))
      {
        if (( M.read(r ,c)= 1 ) & ( M.read(r-1 ,c)= 1 ) & ( M.read(r ,c-1)= 1 ) & ( M.read(r, c+1)= 1 ))
          {let type= 2;}
      }

      if ((r> 0) & (r< 19) & (c< 9))
      {
        if (( M.read(r ,c)= 1 ) & ( M.read(r-1 ,c)= 1 ) & ( M.read(r ,c+1)= 1 ) & ( M.read(r+1 ,c)= 1 ))
          {let type= 3;}
      }

      if (type= 0)
	{
		// test 1
		if ( r> 0 )
		{
			if ( M.read(r-1, c)= 0 )
			{
				do M.set(r-1, c);
				do M.reset(r, c+1);
				let type = 1;
				return true;
			}
		}

		// test 2
		if ( r> 0 )
		{
			if ( (M.read(r-1, c+1)= 0) & (M.read(r+1, c+1)= 0) )
			{
				do M.set(r-1, c+1);
				do M.set(r+1, c+1);
				do M.reset(r, c-1);
				do M.reset(r+1, c);
				
				do B.setC(c+1);
        let type = 1;
				return true;
			}
		}

		// test 3
		if ( r> 1 )
		{
			if ( (M.read(r-1, c+1)= 0) & (M.read(r-1, c)= 0) & (M.read(r-2, c+1)= 0) )
			{
				do M.set(r-1, c+1);
				do M.set(r-1, c);
				do M.set(r-2, c+1);
				do M.reset(r, c-1);
				do M.reset(r, c);
				do M.reset(r+1, c);
				
				do B.setR(r-1);
				do B.setC(c+1);
        let type = 1;
				return true;
			}
		}

		// test 4
		if ( r< 17 )
		{
			if ( (M.read(r+2, c)= 0) & (M.read(r+2, c-1)= 0) & (M.read(r+3, c)= 0) )
			{
				do M.set(r+2, c);
				do M.set(r+2, c-1);
				do M.set(r+3, c);
				do M.reset(r, c-1);
				do M.reset(r, c);
				do M.reset(r, c+1);
				
				do B.setR(r+2);
        let type = 1;
				return true;
			}
		}

		// test 5
		if ( r< 17 )
		{
			if ( (M.read(r+2, c+1)= 0) & (M.read(r+1, c+1)= 0) & (M.read(r+3, c+1)= 0) & (M.read(r+2, c)= 0) )
			{
				do M.set(r+2, c+1);
				do M.set(r+1, c+1);
				do M.set(r+3, c+1);
				do M.set(r+2, c);
				do M.reset(r, c-1);
				do M.reset(r, c);
				do M.reset(r, c+1);
				do M.reset(r+1, c);
				
				do B.setR(r+2);
				do B.setC(c+1);
        let type = 1;
				return true;
			}
		}
    
	}

      if (type= 1)
	{
		// test 1
		if ( c< 9 )
		{
			if ( M.read(r, c+1)= 0 )
			{
				do M.set(r, c+1);
				do M.reset(r+1, c);
        let type = 2;
				return true;
			}
		}

		// test 2
		if ( c> 1 )
		{
			if ( (M.read(r-1, c-1)= 0) & (M.read(r, c-2)= 0) )
			{
				do M.set(r-1, c-1);
				do M.set(r, c-2);
				do M.reset(r-1, c);
				do M.reset(r+1, c);

				do B.setC(c-1);
        let type = 2;
				return true;
			}
		}

		// test 3
		if ( c> 1 )
		{
			if ( (M.read(r+1, c-1)= 0) & (M.read(r+1, c-2)= 0) )
			{
				do M.set(r+1, c-1);
				do M.set(r+1, c-2);
				do M.reset(r, c);
				do M.reset(r-1, c);
				
				do B.setR(r+1);
				do B.setC(c-1);
        let type = 2;
				return true;
			}
		}

		// test 4
		if ( (r> 2) & (c< 9) )
		{
			if ( (M.read(r-2, c)= 0) & (M.read(r-2, c-1)= 0) & (M.read(r-2, c+1)= 0) & (M.read(r-3, c)= 0) )
			{
				do M.set(r-2, c);
				do M.set(r-2, c-1);
				do M.set(r-2, c+1);
				do M.set(r-3, c);
				do M.reset(r, c);
				do M.reset(r, c-1);
				do M.reset(r-1, c);
				do M.reset(r+1, c);

				do B.setR(r-2);
        let type = 2;
				return true;
			}
		}

		// test 5
		if ( (r> 2) & (c> 1) )
		{
			if ( (M.read(r-2, c-1)= 0) & (M.read(r-2, c-2)= 0) & (M.read(r-2, c)= 0) & (M.read(r-3, c-1)= 0) )
			{
				do M.set(r-2, c-1);
				do M.set(r-2, c-2);
				do M.set(r-2, c);
				do M.set(r-3, c-1);
				do M.reset(r, c);
				do M.reset(r, c-1);
				do M.reset(r-1, c);
				do M.reset(r+1, c);

				do B.setR(r-2);
				do B.setC(c-1);
        let type = 2;
				return true;
			}
		}
	}

      if (type= 2)
	{
		// test 1
		if ( r< 19 )
		{
			if ( M.read(r+1, c)= 0 )
			{
				do M.set(r+1, c);
				do M.reset(r, c-1);
        let type = 3;
				return true;
			}
		}

		// test 2
		if ( c> 1 )
		{
			if ( (M.read(r-1, c-1)= 0) & (M.read(r+1, c-1)= 0) )
			{
				do M.set(r-1, c-1);
				do M.set(r+1, c-1);
				do M.reset(r-1, c);
				do M.reset(r, c+1);

				do B.setC(c-1);
        let type = 3;
				return true;
			}
		}

		// test 3
		if ( r> 1 )
		{
			if ( (M.read(r-1, c-1)= 0) & (M.read(r-2, c-1)= 0) )
			{
				do M.set(r-1, c-1);
				do M.set(r-2, c-1);
				do M.reset(r, c);
				do M.reset(r, c+1);
				
				do B.setR(r-1);
				do B.setC(c-1);
        let type = 3;
				return true;
			}
		}

		// test 4
		if ( r< 17 )
		{
			if ( (M.read(r+2, c)= 0) & (M.read(r+2, c+1)= 0) & (M.read(r+1, c)= 0) & (M.read(r+3, c)= 0) )
			{
				do M.set(r+2, c);
				do M.set(r+2, c+1);
				do M.set(r+1, c);
				do M.set(r+3, c);
				do M.reset(r, c);
				do M.reset(r, c-1);
				do M.reset(r, c+1);
				do M.reset(r-1, c);
				
				do B.setR(r+2);
        let type = 3;
				return true;
			}
		}

		// test 5
		if ( r< 17 )
		{
			if ( (M.read(r+2, c-1)= 0) & (M.read(r+2, c)= 0) & (M.read(r+1, c-1)= 0) & (M.read(r+3, c-1)= 0) )
			{
				do M.set(r+2, c-1);
				do M.set(r+2, c);
				do M.set(r+1, c-1);
				do M.set(r+3, c-1);
				do M.reset(r, c);
				do M.reset(r, c-1);
				do M.reset(r, c+1);
				do M.reset(r-1, c);
				
				do B.setR(r+2);
				do B.setC(c-1);
        let type = 3;
				return true;
			}
		}
	}


      if (type= 3)
	{
		// test 1
		if ( c> 0 )
		{
			if ( (M.read(r, c-1)= 0) )
			{
				do M.set(r, c-1);
				do M.reset(r-1, c);
        let type = 0;
				return true;
			}
		}

		// test 2
		if ( c< 8 )
		{
			if ( (M.read(r, c+2)= 0) & (M.read(r+1, c+1)= 0) )
			{
				do M.set(r, c+2);
				do M.set(r+1, c+1);
				do M.reset(r-1, c);
				do M.reset(r+1, c);

				do B.setC(c+1);
        let type = 0;
				return true;
			}
		}

		// test 3
		if ( (r< 18) & (c< 8) )
		{
			if ( (M.read(r+1, c+1)= 0) & (M.read(r+1, c+2)= 0) & (M.read(r+2, c+1)= 0) )
			{
				do M.set(r+1, c+1);
				do M.set(r+1, c+2);
				do M.set(r+2, c+1);
				do M.reset(r, c);
				do M.reset(r, c+1);
				do M.reset(r-1, c);
				
				do B.setR(r+1);
				do B.setC(c+1);
        let type = 0;
				return true;
			}
		}

		// test 4
		if ( (r> 1) & (c> 0) )
		{
			if ( (M.read(r-2, c)= 0) & (M.read(r-2, c-1)= 0) & (M.read(r-2, c+1)= 0) )
			{
				do M.set(r-2, c);
				do M.set(r-2, c-1);
				do M.set(r-2, c+1);
				do M.reset(r, c);
				do M.reset(r+1, c);
				do M.reset(r, c+1);
				
				do B.setR(r-2);
        let type = 0;
				return true;
			}
		}

		// test 5
		if ( (r> 1) & (c< 8) )
		{
			if ( (M.read(r-2, c+1)= 0) & (M.read(r-1, c+1)= 0) & (M.read(r-2, c)= 0) & (M.read(r-2, c+2)= 0) )
			{
				do M.set(r-2, c+1);
				do M.set(r-1, c+1);
				do M.set(r-2, c);
				do M.set(r-2, c+2);
				do M.reset(r, c);
				do M.reset(r-1, c);
				do M.reset(r+1, c);
				do M.reset(r, c+1);
				
				do B.setR(r-2);
				do B.setC(c+1);
        let type = 0;
				return true;
			}
		}
    }
    return false;
    }

    method void down(Map M,Block B)
    {
      var int r;
      var int c;
      let r = B.getR();
      let c = B.getC();

      if (type = 0)
      {
        if (r<18)
        {
          if ((M.read(r+1,c-1)=1)|(M.read(r+2,c)=1)|(M.read(r+1,c+1)=1))
          {
            return ;
          }
          else
          {
            do M.reset(r,c-1);
            do M.reset(r,c);
            do M.reset(r,c+1);
            do M.set(r+1,c-1);
            do M.set(r+2,c);
            do M.set(r+1,c+1);

            do B.setR(r+1);
          }
        }
        return ;
      }
      if (type = 1)
      {
        if (r<18)
        {
          if ((M.read(r+1,c-1)=1)|(M.read(r+2,c)=1))
          {
            return ;
          }
          else
          {
            do M.reset(r-1,c);
            do M.reset(r,c-1);
            do M.set(r+1,c-1);
            do M.set(r+2,c);

            do B.setR(r+1);
          }
        }
        return;
      }
      if (type = 2)
      {
        if (r<19)
        {
          if ((M.read(r+1,c-1)=1)|(M.read(r+1,c)=1)|(M.read(r+1,c+1)=1))
          {
            return ;
          }
          else
          {
            do M.reset(r,c-1);
            do M.reset(r-1,c);
            do M.reset(r,c+1);
            do M.set(r+1,c-1);
            do M.set(r+1,c);
            do M.set(r+1,c+1);

            do B.setR(r+1);
          }
        }
        return;
      }

      if (type = 3)
      {
        if (r<18)
        {
          if ((M.read(r+2,c)=1)|(M.read(r+1,c+1)=1))
          {
            return;
          }
          else
          {
            do M.reset(r-1,c);
            do M.reset(r,c+1);
            do M.set(r+2,c);
            do M.set(r+1,c+1);

            do B.setR(r+1);
          }
        }
        return ;
      }

      return;
    }

    method void left(Map M, Block B)
{
	
        var int r; 
	      var int c; 
        let r= B.getR();
        let c= B.getC();

	// verdict type 0 or 1 or 2 or 3
	// here
	
	if (type= 0)
	{
		if (c= 1)
		{
			return;
		}

		if ( (M.read(r, c-2)= 0) & (M.read(r+1, c-1)= 0) )
		{
			do M.set(r, c-2);
			do M.set(r+1, c-1);
			do M.reset(r, c+1);
			do M.reset(r+1, c);

			do B.setC(c-1);
		}
	}

	if (type= 1)
	{
		if (c= 1)
		{
			return;
		}

		if ( (M.read(r-1, c-1)= 0) & (M.read(r, c-2)= 0) & (M.read(r+1, c-1)= 0) )
		{
			do M.set(r-1, c-1);
			do M.set(r, c-2);
			do M.set(r+1, c-1);
			do M.reset(r-1, c);
			do M.reset(r, c);
			do M.reset(r+1, c);

			do B.setC(c-1);
		}
	}

	if (type= 2)
	{
		if (c= 1)
		{
			return;
		}

		if ( (M.read(r-1, c-1)= 0) & (M.read(r, c-2)= 0) )
		{
			do M.set(r-1, c-1);
			do M.set(r, c-2);
			do M.reset(r-1, c);
			do M.reset(r, c+1);

			do B.setC(c-1);
		}
	}

	if (type= 3)
	{
		if (c= 0)
		{
			return;
		}

		if ( (M.read(r-1, c-1)= 0) & (M.read(r, c-1)= 0) & (M.read(r+1, c-1)= 0) )
		{
			do M.set(r-1, c-1);
			do M.set(r, c-1);
			do M.set(r+1, c-1);
			do M.reset(r-1, c);
			do M.reset(r, c+1);
			do M.reset(r+1, c);

			do B.setC(c-1);
		}
	}

	return;	
}

method void right(Map M, Block B)
{
	
      var int r; 
      var int c; 
      let r= B.getR();
      let c= B.getC();

      // verdict type 0 or 1 or 2 or 3
      // here
      
      if (type= 0)
      {
        if (c= 8)
        {
          return;
        }

        if ( (M.read(r, c+2)= 0) & (M.read(r+1, c+1)= 0) )
        {
          do M.set(r, c+2);
          do M.set(r+1, c+1);
          do M.reset(r, c-1);
          do M.reset(r+1, c);

          do B.setC(c+1);
        }
      }

      if (type= 1)
      {
        if (c= 9)
        {
          return;
        }

        if ( (M.read(r-1, c+1)= 0) & (M.read(r, c+1)= 0) & (M.read(r+1, c+1)= 0) )
        {
          do M.set(r-1, c+1);
          do M.set(r, c+1);
          do M.set(r+1, c+1);
          do M.reset(r-1, c);
          do M.reset(r, c-1);
          do M.reset(r+1, c);

          do B.setC(c+1);
        }
      }

      if (type= 2)
      {
        if (c= 8)
        {
          return;
        }

        if ( (M.read(r-1, c+1)= 0) & (M.read(r, c+2)= 0) )
        {
          do M.set(r-1, c+1);
          do M.set(r, c+2);
          do M.reset(r-1, c);
          do M.reset(r, c-1);

          do B.setC(c+1);
        }
      }

      if (type= 3)
      {
        if (c= 8)
        {
          return;
        }

        if ( (M.read(r-1, c+1)= 0) & (M.read(r, c+2)= 0) & (M.read(r+1, c+1)= 0) )
        {
          do M.set(r-1, c+1);
          do M.set(r, c+2);
          do M.set(r+1, c+1);
          do M.reset(r-1, c);
          do M.reset(r, c);
          do M.reset(r+1, c);

          do B.setC(c+1);
        }
      }
      
      return;		
    }

    method boolean T_isBottom(Map M, Block B)
    {
      var int r;
      var int c;
      var boolean isbottom;
      let r = B.getR();
      let c = B.getC();

      if (type = 0)
      {
        if (r<18)
        {
          if ((M.read(r+1,c-1)=1)|(M.read(r+2,c)=1)|(M.read(r+1,c+1)=1))
          {
            let isbottom = true;
          }
          else
          {
            let isbottom = false;
          }
        }
        else
        {
            let isbottom = true;
        }
      }
      if (type = 1)
      {
        if (r<18)
        {
          if ((M.read(r+1,c-1)=1)|(M.read(r+2,c)=1))
          {
            let isbottom = true;
          }
          else
          {
            let isbottom = false;
          }
        }
        else
        {
            let isbottom = true;
        }
      }
      if (type = 2)
      {
        if (r<19)
        {
          if ((M.read(r+1,c-1)=1)|(M.read(r+1,c)=1)|(M.read(r+1,c+1)=1))
          {
            let isbottom = true;
          }
          else
          {
            let isbottom = false;
          }
        }
        else
        {
          let isbottom = true;
        }
      }
      if (type = 3)
      {
        if (r<18)
        {
          if ((M.read(r+2,c)=1)|(M.read(r+1,c+1)=1))
          {
            let isbottom = true;
          }
          else
          {
            let isbottom = false;
          }
        }
        else
        {
            let isbottom = true;
        }
      }
      return isbottom;
    }

